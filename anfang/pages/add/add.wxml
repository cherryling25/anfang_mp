    <view class="page__bd">
        <mp-cells>
            <mp-cell>
                <mp-uploader bindfail="uploadError" bindsuccess="uploadSuccess" select="{{selectFile}}" upload="{{uplaodFile}}" files="{{files}}" max-count="1"></mp-uploader>
            </mp-cell>
        </mp-cells>
    </view>
   
  <view class="button" bindtap="submit">提交</view>
</view>