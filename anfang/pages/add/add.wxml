<view>
  <view class="input_info">
    <text>人员编号</text>
    <input class="name" name="name" bindinput="inputInfo" data-obj="info" data-item="number" value="{{info.number}}" maxlength="12" placeholder="请输入编号" placeholder-class="input_pla" />
  </view>
    <view class="input_info">
    <text>人员姓名</text>
    <input class="name" name="name" bindinput="inputInfo" data-obj="info" data-item="name" value="{{info.name}}" maxlength="5" placeholder="请输入姓名" placeholder-class="input_pla" />
  </view>
    <view class="input_info">
    <text>性别</text>
    <input class="name" name="name" bindinput="inputInfo" data-obj="info" data-item="sex" value="{{info.sex}}" maxlength="3" placeholder="请输入性别" placeholder-class="input_pla" />
  </view>
   <view class="input_info">
    <text>手机号</text>
    <input class="name" name="name" bindinput="inputInfo" data-obj="info" data-item="phone" value="{{info.phone}}" maxlength="11" placeholder="请输入手机号" placeholder-class="input_pla" />
  </view>
   <view class="input_info">
    <text>身份证号</text>
    <input class="name" name="name" bindinput="inputInfo" data-obj="info" data-item="idNumber" value="{{info.idNumber}}" maxlength="18" placeholder="请输入身份证号" placeholder-class="input_pla" />
  </view>
    <view class="input_info">
    <text>地址</text>
    <input class="name" name="name" bindinput="inputInfo" data-obj="info" data-item="address" value="{{info.address}}" maxlength="12" placeholder="请输入地址" placeholder-class="input_pla" />
  </view>
   <view class="input_info">
    <text>部门</text>
    <picker bindchange="bindPickerChange" value="{{info.department}}" range="{{array}}">
    <view class="picker">
      请选择：{{info.department}}
    </view>
  </picker>
  </view>
   <view class="input_info">
    <text>到期时间</text>
    <picker mode="date" value="{{info.daoqiTime}}" start="2015-09-01" end="2017-09-01" bindchange="bindDateChange">
    <view class="picker">
      请选择: {{info.daoqiTime}}
    </view>
  </picker>
