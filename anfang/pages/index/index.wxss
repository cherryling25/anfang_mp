.banner-box {
  width: 100%;
  height: 400rpx;
  padding: 0;
}

.banner-box .banner-list {
  height: 400rpx;
  padding: 0;
}

.banner-box .banner-list .swiper {
  height: 400rpx;
  padding: 0;
}

.banner-box .swiper swiper-item {
  display: block;
  overflow: hidden;
  will-change: transform;
  position: absolute;
  width: 100%;
  height: 100%;
}

.banner-box .swiper .swiper-item {
  display: block;
  height: 400rpx;
  line-height: 400rpx;
  text-align: center;
  color: #fff;
}

.banner-box .swiper .swiper-item image {
  width: 100%;
  height: 400rpx;
  margin: auto;
}

.operate {
  display: flex;
  align-items: center;
  height: 200rpx;
  margin-top: 10rpx;
  border-top: 1rpx solid #eee;
  border-bottom: 1rpx solid #eee;
}

.operate_one {
  width: 33%;
  display: flex;
  flex-direction: column;
  align-items: center;
 justify-content: center;
  height: 100%;
  line-height: 100%;
}

.operate_one image {
  height: 80rpx;
  width: 80rpx;
  margin-bottom: 10rpx;
}

.line {
  border-right: 1rpx solid #eee;
  border-left: 1rpx solid #eee;
}
.operate_one view{
    margin-top: 10rpx;
    color: #5e5e5e;
}
.fixed{
  position: fixed;
  width: 100%;
  display: flex;
  justify-content: center;
  bottom:10%;
}
.fixed image{
  width: 200rpx;
  height: 200rpx;
}